
<div class="row profile">
  <div class="col s4 offset-s4">
    <% @user.posts.all.reverse.each do |post| %>
      <div class="card">
        <div class="card-content center-align">
          <h3><%= @user.username %></h3>
          <h4><%= post.title %></h4>
          <h5><%= post.content %></h5>
        </div>
      </div>
    <% end %>
  </div>
</div>
