
<div class="row home-page">
  <div class="col s4 offset-s4">
    <% @posts.reverse.each do |post| %>
      <div class="card">
        <div class="card-content center-align">
          <h3><%= User.find(post.user_id).username %></h3>
          <h4><%= post.title %></h4>
          <h5><%= post.content %></h5>
        </div>
      </div>
    <% end %>
  </div>
</div>